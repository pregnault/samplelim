        RespSEPATL      = respirationSEPATL
        RespZOOPLANKT   = respirationZOOPLANKT
        RespPHYTOPLANKT = respirationPHYTOPLANKT
        !RespSEDBAC      = respirationSEDBAC
        !RespWATBAC      = respirationWATBAC

        RespBenth       = avgSedResp


        !-- Plankton and bacteria
        !These do not need mass balances as they only consume one source.

        !- Various
        SOMbur = SOMburial
        BACbur = BACburial

#### END EQUATION



#### CONSTRAINTS

    !- Physiological constraints
    !-- Fish
    !--- Trachurus trachurus
    AssiTRATRA = [minAE, maxAE]*UptaTRATRA
