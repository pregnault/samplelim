        NEPHTY->CALLYR*IsoNEPHTY   + CARPOLP->CALLYR*IsoCARPOLP   + & 
        CARPOLC->CALLYR*IsoCARPOLC + CARPOLPC->CALLYR*IsoCARPOLPC + & 
        SUSWOR->CALLYR*IsoSUSWOR   + SUSBIV->CALLYR*IsoSUSBIV     + & 
        HSUDEBI->CALLYR*IsoHSUDEBI + SUDEBI->CALLYR*IsoSUDEBI     + & 
        BROGAS->CALLYR*IsoBROGAS   + SUSGAS->CALLYR*IsoSUSGAS     + & 
        ZOOPLANKT->CALLYR*IsoZOOPLANKT                            + &
        DETAMP->CALLYR*FracCALLYR  + DPAMP->CALLYR*FracCALLYR    + &
        DETSHR->CALLYR*FracCALLYR  + SUDECR->CALLYR*FracCALLYR   + &
        PAGURI->CALLYR*FracCALLYR  + CARNCR->CALLYR*FracCALLYR   + &
        OPHIUR->CALLYR*FracCALLYR  + DETPOL->CALLYR*FracCALLYR   + &
        NEPHTY->CALLYR*FracCALLYR  + CARPOLP->CALLYR*FracCALLYR  + &
        CARPOLC->CALLYR*FracCALLYR + CARPOLPC->CALLYR*FracCALLYR + &
        SUSWOR->CALLYR*FracCALLYR  + SUSBIV->CALLYR*FracCALLYR   + &
        HSUDEBI->CALLYR*FracCALLYR + SUDEBI->CALLYR*FracCALLYR   + &
        BROGAS->CALLYR*FracCALLYR  + SUSGAS->CALLYR*FracCALLYR   + &
        ZOOPLANKT->CALLYR*FracCALLYR

        !-- Jellyfish
        ZOOPLANKT->JELLY*IsoJELLY     + SPOM->JELLY*IsoJELLY  = &
        ZOOPLANKT->JELLY*IsoZOOPLANKT + SPOM->JELLY*IsoSPOM   + &
        ZOOPLANKT->JELLY*FracJELLY    + SPOM->JELLY*FracJELLY

        !-- Cumaceans
        PHYTOPLANKT->CUMACE*IsoCUMACE + ZOOPLANKT->CUMACE*IsoCUMACE + &
        SOM->CUMACE*IsoCUMACE         + SPOM->CUMACE*IsoCUMACE      = &
        PHYTOPLANKT->CUMACE*IsoPHYTOPLANKT + ZOOPLANKT->CUMACE*IsoZOOPLANKT + &
        SOM->CUMACE*IsoSOM                 + SPOM->CUMACE*IsoSPOM           + &
        PHYTOPLANKT->CUMACE*FracCUMACE + ZOOPLANKT->CUMACE*FracCUMACE + &
        SOM->CUMACE*FracCUMACE         + SPOM->CUMACE*FracCUMACE

        !-- Callianassidae 
        SOM->CALLIA*IsoCALLIA + SPOM->CALLIA*IsoCALLIA = &
        SOM->CALLIA*IsoSOM    + SPOM->CALLIA*IsoSPOM   + &
        SOM->CALLIA*FracCALLIA   + SPOM->CALLIA*FracCALLIA

        !-- Detritivore amphipods : Balance not needed for only 1 foodsource
        !SOM->DETAMP 

        !-- Detritivore-planktivore amphipods
        ZOOPLANKT->DPAMP*IsoDPAMP     + SOM->DPAMP*IsoDPAMP  = &
        ZOOPLANKT->DPAMP*IsoZOOPLANKT + SOM->DPAMP*IsoSOM    + &
        ZOOPLANKT->DPAMP*FracDPAMP    + SOM->DPAMP*FracDPAMP
        
        !-- Detritivore shrimp
        ZOOPLANKT->DETSHR*IsoDETSHR    + SPOM->DETSHR*IsoDETSHR + &
        SOM->DETSHR*IsoDETSHR                                   = &
        ZOOPLANKT->DETSHR*IsoZOOPLANKT + SPOM->DETSHR*IsoSPOM + &
        SOM->DETSHR*IsoSOM                                    + &
        ZOOPLANKT->DETSHR*FracDETSHR + SPOM->DETSHR*FracDETSHR + &
        SOM->DETSHR*FracDETSHR

        !-- Small suspension-deposit feeding crabs
        PHYTOPLANKT->SUDECR*IsoSUDECR + ZOOPLANKT->SUDECR*IsoSUDECR + &
        SOM->SUDECR*IsoSUDECR         + SPOM->SUDECR*IsoSUDECR      = &
        PHYTOPLANKT->SUDECR*IsoPHYTOPLANKT + ZOOPLANKT->SUDECR*IsoZOOPLANKT + &
        SOM->SUDECR*IsoSOM                 + SPOM->SUDECR*IsoSPOM           + &
        PHYTOPLANKT->SUDECR*FracSUDECR + ZOOPLANKT->SUDECR*FracSUDECR + &
        SOM->SUDECR*FracSUDECR         + SPOM->SUDECR*FracSUDECR

        !-- Pagurids
        ZOOPLANKT->PAGURI*IsoPAGURI    + SPOM->PAGURI*IsoPAGURI + &
        SOM->PAGURI*IsoPAGURI                                   = &
        ZOOPLANKT->PAGURI*IsoZOOPLANKT + SPOM->PAGURI*IsoSPOM   + &
        SOM->PAGURI*IsoSOM                                      + &
        ZOOPLANKT->PAGURI*FracPAGURI + SPOM->PAGURI*FracPAGURI  + &
        SOM->PAGURI*FracPAGURI

        !-- Carnivorous crabs
        POMSP->CARNCR*IsoCARNCR   + CUMACE->CARNCR*IsoCARNCR   + &
        DETAMP->CARNCR*IsoCARNCR  + DPAMP->CARNCR*IsoCARNCR    + &
        DETSHR->CARNCR*IsoCARNCR  + SUDECR->CARNCR*IsoCARNCR   + &
        PAGURI->CARNCR*IsoCARNCR  + DETPOL->CARNCR*IsoCARNCR   + &
        NEPHTY->CARNCR*IsoCARNCR  + CARPOLP->CARNCR*IsoCARNCR  + &
        CARPOLC->CARNCR*IsoCARNCR + CARPOLPC->CARNCR*IsoCARNCR + &
        SUSWOR->CARNCR*IsoCARNCR  + SUSBIV->CARNCR*IsoCARNCR   + &
        HSUDEBI->CARNCR*IsoCARNCR + SUDEBI->CARNCR*IsoCARNCR   + &
        BROGAS->CARNCR*IsoCARNCR  + SUSGAS->CARNCR*IsoCARNCR   + &
        SOM->CARNCR*IsoCARNCR                                  = &
        POMSP->CARNCR*IsoPOMSP     + CUMACE->CARNCR*IsoCUMACE     + &
        DETAMP->CARNCR*IsoDETAMP   + DPAMP->CARNCR*IsoDPAMP       + &
        DETSHR->CARNCR*IsoDETSHR   + SUDECR->CARNCR*IsoSUDECR     + &
        PAGURI->CARNCR*IsoPAGURI   + DETPOL->CARNCR*IsoDETPOL     + &
        NEPHTY->CARNCR*IsoNEPHTY   + CARPOLP->CARNCR*IsoCARPOLP   + &
        CARPOLC->CARNCR*IsoCARPOLC + CARPOLPC->CARNCR*IsoCARPOLPC + &
        SUSWOR->CARNCR*IsoSUSWOR   + SUSBIV->CARNCR*IsoSUSBIV     + &
        HSUDEBI->CARNCR*IsoHSUDEBI + SUDEBI->CARNCR*IsoSUDEBI     + &
        BROGAS->CARNCR*IsoBROGAS   + SUSGAS->CARNCR*IsoSUSGAS     + &
        SOM->CARNCR*IsoSOM                                        + &
        POMSP->CARNCR*FracCARNCR  + CUMACE->CARNCR*FracCARNCR    + &
        DETAMP->CARNCR*FracCARNCR  + DPAMP->CARNCR*FracCARNCR    + &
        DETSHR->CARNCR*FracCARNCR  + SUDECR->CARNCR*FracCARNCR   + &
        PAGURI->CARNCR*FracCARNCR  + DETPOL->CARNCR*FracCARNCR   + &
        NEPHTY->CARNCR*FracCARNCR  + CARPOLP->CARNCR*FracCARNCR  + &
        CARPOLC->CARNCR*FracCARNCR + CARPOLPC->CARNCR*FracCARNCR + &
        SUSWOR->CARNCR*FracCARNCR  + SUSBIV->CARNCR*FracCARNCR   + &
        HSUDEBI->CARNCR*FracCARNCR + SUDEBI->CARNCR*FracCARNCR   + &
        BROGAS->CARNCR*FracCARNCR  + SUSGAS->CARNCR*FracCARNCR   + &
        SOM->CARNCR*FracCARNCR

        !-- Asterias rubens
        CUMACE->ASTRUB*IsoASTRUB  + DETAMP->ASTRUB*IsoASTRUB   + & 
        DPAMP->ASTRUB*IsoASTRUB   + SUDECR->ASTRUB*IsoASTRUB   + & 
        PAGURI->ASTRUB*IsoASTRUB  + CARNCR->ASTRUB*IsoASTRUB   + & 
        ASTRUB->ASTRUB*IsoASTRUB  + DETPOL->ASTRUB*IsoASTRUB   + & 
        NEPHTY->ASTRUB*IsoASTRUB  + CARPOLP->ASTRUB*IsoASTRUB  + & 
        CARPOLC->ASTRUB*IsoASTRUB + CARPOLPC->ASTRUB*IsoASTRUB + & 
        SUSWOR->ASTRUB*IsoASTRUB  + SUSBIV->ASTRUB*IsoASTRUB   + & 
        HSUDEBI->ASTRUB*IsoASTRUB + SUDEBI->ASTRUB*IsoASTRUB   + & 
        BROGAS->ASTRUB*IsoASTRUB  + SUSGAS->ASTRUB*IsoASTRUB   + & 
        SOM->ASTRUB*IsoASTRUB                                  = & 
        CUMACE->ASTRUB*IsoCUMACE   + DETAMP->ASTRUB*IsoDETAMP     + & 
        DPAMP->ASTRUB*IsoDPAMP     + SUDECR->ASTRUB*IsoSUDECR     + & 
        PAGURI->ASTRUB*IsoPAGURI   + CARNCR->ASTRUB*IsoCARNCR     + & 
        ASTRUB->ASTRUB*IsoASTRUB   + DETPOL->ASTRUB*IsoDETPOL     + & 
        NEPHTY->ASTRUB*IsoNEPHTY   + CARPOLP->ASTRUB*IsoCARPOLP   + & 
        CARPOLC->ASTRUB*IsoCARPOLC + CARPOLPC->ASTRUB*IsoCARPOLPC + & 
        SUSWOR->ASTRUB*IsoSUSWOR   + SUSBIV->ASTRUB*IsoSUSBIV     + & 
        HSUDEBI->ASTRUB*IsoHSUDEBI + SUDEBI->ASTRUB*IsoSUDEBI     + & 
        BROGAS->ASTRUB*IsoBROGAS   + SUSGAS->ASTRUB*IsoSUSGAS     + & 
        SOM->ASTRUB*IsoSOM                                        + &
        CUMACE->ASTRUB*FracASTRUB  + DETAMP->ASTRUB*FracASTRUB   + &
        DPAMP->ASTRUB*FracASTRUB   + SUDECR->ASTRUB*FracASTRUB   + &
        PAGURI->ASTRUB*FracASTRUB  + CARNCR->ASTRUB*FracASTRUB   + &
        ASTRUB->ASTRUB*FracASTRUB  + DETPOL->ASTRUB*FracASTRUB   + &
        NEPHTY->ASTRUB*FracASTRUB  + CARPOLP->ASTRUB*FracASTRUB  + &
        CARPOLC->ASTRUB*FracASTRUB + CARPOLPC->ASTRUB*FracASTRUB + &
        SUSWOR->ASTRUB*FracASTRUB  + SUSBIV->ASTRUB*FracASTRUB   + &
        HSUDEBI->ASTRUB*FracASTRUB + SUDEBI->ASTRUB*FracASTRUB   + &
        BROGAS->ASTRUB*FracASTRUB  + SUSGAS->ASTRUB*FracASTRUB   + &
        SOM->ASTRUB*FracASTRUB

        !-- Ophiurids
        CUMACE->OPHIUR*IsoOPHIUR   + DETAMP->OPHIUR*IsoOPHIUR  + &
        DPAMP->OPHIUR*IsoOPHIUR    + DETSHR->OPHIUR*IsoOPHIUR  + &
        OPHIUR->OPHIUR*IsoOPHIUR   + ECHCOR->OPHIUR*IsoOPHIUR  + &
        DETPOL->OPHIUR*IsoOPHIUR   + NEPHTY->OPHIUR*IsoOPHIUR  + &
        CARPOLP->OPHIUR*IsoOPHIUR  + CARPOLC->OPHIUR*IsoOPHIUR + &
        CARPOLPC->OPHIUR*IsoOPHIUR + SUSWOR->OPHIUR*IsoOPHIUR  + &
        SUSBIV->OPHIUR*IsoOPHIUR   + HSUDEBI->OPHIUR*IsoOPHIUR + &
        SUDEBI->OPHIUR*IsoOPHIUR   + BROGAS->OPHIUR*IsoOPHIUR  + &
        SUSGAS->OPHIUR*IsoOPHIUR   + SOM->OPHIUR*IsoOPHIUR     + &
        SPOM->OPHIUR*IsoOPHIUR                                 = &
        CUMACE->OPHIUR*IsoCUMACE + DETAMP->OPHIUR*IsoDETAMP     + &
        DPAMP->OPHIUR*IsoDPAMP + DETSHR->OPHIUR*IsoDETSHR       + &
        OPHIUR->OPHIUR*IsoOPHIUR + ECHCOR->OPHIUR*IsoECHCOR     + &
        DETPOL->OPHIUR*IsoDETPOL + NEPHTY->OPHIUR*IsoNEPHTY     + &
        CARPOLP->OPHIUR*IsoCARPOLP + CARPOLC->OPHIUR*IsoCARPOLC + &
        CARPOLPC->OPHIUR*IsoCARPOLPC + SUSWOR->OPHIUR*IsoSUSWOR + &
        SUSBIV->OPHIUR*IsoSUSBIV + HSUDEBI->OPHIUR*IsoHSUDEBI   + &
        SUDEBI->OPHIUR*IsoSUDEBI + BROGAS->OPHIUR*IsoBROGAS     + &
        SUSGAS->OPHIUR*IsoSUSGAS + SOM->OPHIUR*IsoSOM           + &
        SPOM->OPHIUR*IsoSPOM                                    + &
        CUMACE->OPHIUR*FracOPHIUR   + DETAMP->OPHIUR*FracOPHIUR  + &
        DPAMP->OPHIUR*FracOPHIUR    + DETSHR->OPHIUR*FracOPHIUR  + &
        OPHIUR->OPHIUR*FracOPHIUR   + ECHCOR->OPHIUR*FracOPHIUR  + &
        DETPOL->OPHIUR*FracOPHIUR  + NEPHTY->OPHIUR*FracOPHIUR  + &
        CARPOLP->OPHIUR*FracOPHIUR  + CARPOLC->OPHIUR*FracOPHIUR + &
        CARPOLPC->OPHIUR*FracOPHIUR + SUSWOR->OPHIUR*FracOPHIUR  + &
        SUSBIV->OPHIUR*FracOPHIUR   + HSUDEBI->OPHIUR*FracOPHIUR + &
        SUDEBI->OPHIUR*FracOPHIUR   + BROGAS->OPHIUR*FracOPHIUR  + &
        SUSGAS->OPHIUR*FracOPHIUR   + SOM->OPHIUR*FracOPHIUR     + &
        SPOM->OPHIUR*FracOPHIUR

        !--- Polychaete and crustacea
        CUMACE->CARPOLPC*IsoCARPOLPC   + DETAMP->CARPOLPC*IsoCARPOLPC  + &
        DPAMP->CARPOLPC*IsoCARPOLPC    + DETSHR->CARPOLPC*IsoCARPOLPC  + &
        DETPOL->CARPOLPC*IsoCARPOLPC   + NEPHTY->CARPOLPC*IsoCARPOLPC  + &
        CARPOLP->CARPOLPC*IsoCARPOLPC  + CARPOLC->CARPOLPC*IsoCARPOLPC + &
        CARPOLPC->CARPOLPC*IsoCARPOLPC + SUSWOR->CARPOLPC*IsoCARPOLPC  + &
        SOM->CARPOLPC*IsoCARPOLPC                                      = &
        CUMACE->CARPOLPC*IsoCUMACE     + DETAMP->CARPOLPC*IsoDETAMP   + &
        DPAMP->CARPOLPC*IsoDPAMP       + DETSHR->CARPOLPC*IsoDETSHR   + &
        DETPOL->CARPOLPC*IsoDETPOL     + NEPHTY->CARPOLPC*IsoNEPHTY   + &
        CARPOLP->CARPOLPC*IsoCARPOLP   + CARPOLC->CARPOLPC*IsoCARPOLC + &
        CARPOLPC->CARPOLPC*IsoCARPOLPC + SUSWOR->CARPOLPC*IsoSUSWOR   + &
        SOM->CARPOLPC*IsoSOM                                          + &
        CUMACE->CARPOLPC*FracCARPOLPC   + DETAMP->CARPOLPC*FracCARPOLPC  + &
        DPAMP->CARPOLPC*FracCARPOLPC    + DETSHR->CARPOLPC*FracCARPOLPC  + &
        DETPOL->CARPOLPC*FracCARPOLPC   + NEPHTY->CARPOLPC*FracCARPOLPC  + &
        CARPOLP->CARPOLPC*FracCARPOLPC  + CARPOLC->CARPOLPC*FracCARPOLPC + &
        CARPOLPC->CARPOLPC*FracCARPOLPC + SUSWOR->CARPOLPC*FracCARPOLPC  + &
        SOM->CARPOLPC*FracCARPOLPC

        !-- Suspension feeding polychaetes / phoronids
        ZOOPLANKT->SUSWOR*IsoSUSWOR + PHYTOPLANKT->SUSWOR*IsoSUSWOR + &
        SPOM->SUSWOR*IsoSUSWOR                                      = &
        ZOOPLANKT->SUSWOR*IsoZOOPLANKT + PHYTOPLANKT->SUSWOR*IsoPHYTOPLANKT + &
        SPOM->SUSWOR*IsoSPOM                                                + &
        ZOOPLANKT->SUSWOR*FracSUSWOR + PHYTOPLANKT->SUSWOR*FracSUSWOR + &
        SPOM->SUSWOR*FracSUSWOR

        !-- Suspension feeder bivalves
        ZOOPLANKT->SUSBIV*IsoSUSBIV + PHYTOPLANKT->SUSBIV*IsoSUSBIV + &
        SPOM->SUSBIV*IsoSUSBIV                                      = &
        ZOOPLANKT->SUSBIV*IsoZOOPLANKT + PHYTOPLANKT->SUSBIV*IsoPHYTOPLANKT + &
        SPOM->SUSBIV*IsoSPOM                                                + &
        ZOOPLANKT->SUSBIV*FracSUSBIV + PHYTOPLANKT->SUSBIV*FracSUSBIV + &
        SPOM->SUSBIV*FracSUSBIV

        !-- suspension-deposit feeder bivalves
        !--- Herbivorous
        PHYTOPLANKT->HSUDEBI*IsoHSUDEBI + SOM->HSUDEBI*IsoHSUDEBI + &
        SPOM->HSUDEBI*IsoHSUDEBI                                  = &
        PHYTOPLANKT->HSUDEBI*IsoPHYTOPLANKT + SOM->HSUDEBI*IsoSOM + &
        SPOM->HSUDEBI*IsoSPOM                                     + &
        PHYTOPLANKT->HSUDEBI*FracHSUDEBI + SOM->HSUDEBI*FracHSUDEBI + &
        SPOM->HSUDEBI*FracHSUDEBI

        !-- Echinoidea  : Balance not needed for only 1 foodsource
        !SOM->ECHCOR 

        !-- Detritivore polychates : Balance not needed for only 1 foodsource
        !SOM->DETPOL

        !-- Nephtids
        DETPOL->NEPHTY*IsoNEPHTY   + NEPHTY->NEPHTY*IsoNEPHTY  + &
        CARPOLP->NEPHTY*IsoNEPHTY  + CARPOLC->NEPHTY*IsoNEPHTY + &
        CARPOLPC->NEPHTY*IsoNEPHTY + SUSWOR->NEPHTY*IsoNEPHTY  + &
        SUSBIV->NEPHTY*IsoNEPHTY   + HSUDEBI->NEPHTY*IsoNEPHTY + &
        SUDEBI->NEPHTY*IsoNEPHTY   + SOM->NEPHTY*IsoNEPHTY     = &
        DETPOL->NEPHTY*IsoDETPOL   + NEPHTY->NEPHTY*IsoNEPHTY   + &
        CARPOLP->NEPHTY*IsoCARPOLP + CARPOLC->NEPHTY*IsoCARPOLC + &
        CARPOLPC->NEPHTY*CARPOLPC  + SUSWOR->NEPHTY*IsoSUSWOR   + &
        SUSBIV->NEPHTY*IsoSUSBIV   + HSUDEBI->NEPHTY*IsoHSUDEBI + &
        SUDEBI->NEPHTY*IsoSUDEBI   + SOM->NEPHTY*IsoSOM         + &
        DETPOL->NEPHTY*FracNEPHTY   + NEPHTY->NEPHTY*FracNEPHTY  + &
        CARPOLP->NEPHTY*FracNEPHTY  + CARPOLC->NEPHTY*FracNEPHTY + &
        CARPOLPC->NEPHTY*FracNEPHTY + SUSWOR->NEPHTY*FracNEPHTY  + &
        SUSBIV->NEPHTY*FracNEPHTY   + HSUDEBI->NEPHTY*FracNEPHTY + &
        SUDEBI->NEPHTY*FracNEPHTY   + SOM->NEPHTY*FracNEPHTY

        !-- Carnivorous polychaetes subdivided per main prey.
        !--- Polychaete
        DETPOL->CARPOLP*IsoCARPOLP   + NEPHTY->CARPOLP*IsoCARPOLP  + & 
        CARPOLP->CARPOLP*IsoCARPOLP  + CARPOLC->CARPOLP*IsoCARPOLP + & 
        CARPOLPC->CARPOLP*IsoCARPOLP + SUSWOR->CARPOLP*IsoCARPOLP  + & 
        SOM->CARPOLP*IsoCARPOLP                                    = & 
        DETPOL->CARPOLP*IsoDETPOL     + NEPHTY->CARPOLP*IsoNEPHTY   + & 
        CARPOLP->CARPOLP*IsoCARPOLP   + CARPOLC->CARPOLP*IsoCARPOLC + & 
        CARPOLPC->CARPOLP*IsoCARPOLPC + SUSWOR->CARPOLP*IsoSUSWOR   + & 
        SOM->CARPOLP*IsoSOM                                         + & 
        DETPOL->CARPOLP*FracCARPOLP   + NEPHTY->CARPOLP*FracCARPOLP  + &
        CARPOLP->CARPOLP*FracCARPOLP  + CARPOLC->CARPOLP*FracCARPOLP + &
        CARPOLPC->CARPOLP*FracCARPOLP + SUSWOR->CARPOLP*FracCARPOLP  + &
        SOM->CARPOLP*FracCARPOLP

        !--- Crustacea
        CUMACE->CARPOLC*IsoCARPOLC + CARPOLP->CARPOLC*IsoCARPOLC + &
        DPAMP->CARPOLC*IsoCARPOLC  + DETSHR->CARPOLC*IsoCARPOLC  + &
        SOM->CARPOLC*IsoCARPOLC                                  = &
        CUMACE->CARPOLC*IsoCUMACE + CARPOLP->CARPOLC*IsoCARPOLP + &
        DPAMP->CARPOLC*IsoDPAMP   + DETSHR->CARPOLC*IsoDETSHR   + &
        SOM->CARPOLC*IsoSOM                                     + &
        CUMACE->CARPOLC*FracCARPOLC + CARPOLP->CARPOLC*FracCARPOLC + &
        DPAMP->CARPOLC*FracCARPOLC  + DETSHR->CARPOLC*FracCARPOLC  + &
        SOM->CARPOLC*FracCARPOLC

        !--- Zooplankton also
        PHYTOPLANKT->SUDEBI*IsoSUDEBI + ZOOPLANKT->SUDEBI*IsoSUDEBI + &
        SOM->SUDEBI*IsoSUDEBI         + SPOM->SUDEBI*IsoSUDEBI      = &
        PHYTOPLANKT->SUDEBI*IsoPHYTOPLANKT + ZOOPLANKT->SUDEBI*IsoZOOPLANKT + &
        SOM->SUDEBI*IsoSOM                 + SPOM->SUDEBI*IsoSPOM           + &
        PHYTOPLANKT->SUDEBI*FracSUDEBI + ZOOPLANKT->SUDEBI*FracSUDEBI + &
        SOM->SUDEBI*FracSUDEBI         + SPOM->SUDEBI*FracSUDEBI

        !-- Grazing/browsing gastropods : Mass balance not needed, only 1 source.
        !SOM->BROGAS

        !!-- Suspension-feeder gastropods
        PHYTOPLANKT->SUSGAS*IsoSUSGAS       + SPOM->SUSGAS*IsoSUSGAS       = &
        PHYTOPLANKT->SUSGAS*IsoPHYTOPLANKT  + SPOM->SUSGAS*IsoSPOM         + &
        PHYTOPLANKT->SUSGAS*FracSUSGAS + SPOM->SUSGAS*FracSUSGAS

        !-- Sepiola atlantica
        POMSP->SEPATL*IsoSEPATL  + CUMACE->SEPATL*IsoSEPATL + &
        DETAMP->SEPATL*IsoSEPATL + DPAMP->SEPATL*IsoSEPATL  + &
        DETSHR->SEPATL*IsoSEPATL + SUDECR->SEPATL*IsoSEPATL + &
        PAGURI->SEPATL*IsoSEPATL + CARNCR->SEPATL*IsoSEPATL = &
        POMSP->SEPATL*IsoPOMSP   + CUMACE->SEPATL*IsoCUMACE + &
        DETAMP->SEPATL*IsoDETAMP + DPAMP->SEPATL*IsoDPAMP   + &
        DETSHR->SEPATL*IsoDETSHR + SUDECR->SEPATL*IsoSUDECR + &
        PAGURI->SEPATL*IsoPAGURI + CARNCR->SEPATL*IsoCARNCR + &
        POMSP->SEPATL*FracSEPATL  + CUMACE->SEPATL*FracSEPATL + &
        DETAMP->SEPATL*FracSEPATL + DPAMP->SEPATL*FracSEPATL  + &
        DETSHR->SEPATL*FracSEPATL + SUDECR->SEPATL*FracSEPATL + &
        PAGURI->SEPATL*FracSEPATL + CARNCR->SEPATL*FracSEPATL

!!!!!!!!!!!

    !- Respiration
        RespTRATRA      = respirationTRATRA
        RespARNLAT      = respirationARNLAT
        RespECHVIP      = respirationECHVIP
        RespPLEPLA      = respirationPLEPLA
        RespPOMSP       = respirationPOMSP
        RespCALLYR      = respirationCALLYR
        RespJELLY       = respirationJELLY
        RespCUMACE      = respirationCUMACE
        RespCALLIA      = respirationCALLIA
        RespDETAMP      = respirationDETAMP
        RespDPAMP       = respirationDPAMP
        RespDETSHR      = respirationDETSHR
        RespSUDECR      = respirationSUDECR
        RespPAGURI      = respirationPAGURI
        RespCARNCR      = respirationCARNCR
        RespASTRUB      = respirationASTRUB
        RespOPHIUR      = respirationOPHIUR
        RespECHCOR      = respirationECHCOR
        RespDETPOL      = respirationDETPOL
        RespNEPHTY      = respirationNEPHTY
        RespCARPOLP     = respirationCARPOLP
        RespCARPOLC     = respirationCARPOLC
        RespCARPOLPC    = respirationCARPOLPC
        RespSUSWOR      = respirationSUSWOR
        RespSUSBIV      = respirationSUSBIV
        RespSUDEBI      = respirationSUDEBI
        RespHSUDEBI     = respirationHSUDEBI
