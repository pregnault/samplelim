        !--- Callionymus lyra
        DETAMP->CALLYR*IsoCALLYR  + DPAMP->CALLYR*IsoCALLYR    + &
        DETSHR->CALLYR*IsoCALLYR  + SUDECR->CALLYR*IsoCALLYR   + &
        PAGURI->CALLYR*IsoCALLYR  + CARNCR->CALLYR*IsoCALLYR   + &
        OPHIUR->CALLYR*IsoCALLYR  + DETPOL->CALLYR*IsoCALLYR   + &
        NEPHTY->CALLYR*IsoCALLYR  + CARPOLP->CALLYR*IsoCALLYR  + &
        CARPOLC->CALLYR*IsoCALLYR + CARPOLPC->CALLYR*IsoCALLYR + &
        SUSWOR->CALLYR*IsoCALLYR  + SUSBIV->CALLYR*IsoCALLYR   + &
        HSUDEBI->CALLYR*IsoCALLYR + SUDEBI->CALLYR*IsoCALLYR   + &
        BROGAS->CALLYR*IsoCALLYR  + SUSGAS->CALLYR*IsoCALLYR   + &
        ZOOPLANKT->CALLYR*IsoCALLYR                            = &
